  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "sourceMap": false,
    "rootDir": "./",
    "strict": true,
    "noEmit": true,
    "noImplicitThis": true
